---
import { readdirSync } from 'fs';
import { resolve } from 'path';
import ComponentList from './ComponentList.jsx';

// Lendo os arquivos na pasta de componentes
const componentsDir = resolve('./src/components');
const components = readdirSync(componentsDir).filter(file => file.endsWith('.astro'));
---

<ComponentList components={components} client:load />